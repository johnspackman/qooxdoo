{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source"
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "environment": {
    "qx.icontheme": "Tango"
  },
  "applications": [
    {
      "class": "checkedlist.Application",
      "theme": "qx.theme.TangibleLight",
      "name": "checkedlist",
      "bootPath": "source/boot"
    }
  ],
  "eslintConfig": {
    "extends": [
      "@qooxdoo/qx/browser",
      "@qooxdoo/qx",
      "@qooxdoo/jsdoc-disable"
    ]
  }
}